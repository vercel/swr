{
  "name": "swr",
  "version": "0.4.0",
  "description": "React Hooks library for remote data fetching",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "exports": "dist/index.modern.js",
  "react-native": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "repository": "vercel/swr",
  "homepage": "https://swr.vercel.app",
  "license": "MIT",
  "scripts": {
    "build": "microbundle --no-sourcemap -f modern,es,cjs",
    "watch": "tsc --watch",
    "types:check": "tsc --noEmit",
    "format": "prettier --write \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,test,examples}/**/*.{ts,tsx}\" --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run types:check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "9.4.0",
    "@types/jest": "24.0.20",
    "@types/node": "11.12.0",
    "@types/react": "16.9.11",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "2.4.1",
    "jest": "24.9.0",
    "lint-staged": "8.2.1",
    "microbundle": "0.13.0",
    "prettier": "1.18.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "ts-jest": "24.1.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "dequal": "2.0.2"
  },
  "peerDependencies": {
    "react": "^16.11.0 || ^17.0.0"
  }
}
